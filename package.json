    "tubugl-core": "https://github.com/kenjiSpecial/tubugl-core.git",
    "tubugl-helper": "^1.0.1",
    "tubugl-math": "https://github.com/kenjiSpecial/tubugl-math.git",
    "tubugl-utils": "https://github.com/kenjiSpecial/tubugl-utils.git",
    "wolfy87-eventemitter": "^5.2.4"
  },
